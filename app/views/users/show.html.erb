<h1><%= @user.name%>'s Rooms</h1>


<% @user.rooms.each do |room| %>
    <%= link_to room.name, room_path(room) %>
    <%= link_to "Delete Room", room, method: :delete, data: { confirm: "Do you want to delete this room?" } %><br>
    
<% end %>
<br>
<br>
<%= link_to "Create a Room", new_room_path, class: 'button' %>
